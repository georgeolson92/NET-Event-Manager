@model IEnumerable<EventManager.Models.Event>

<html>
<head>
    <title>Event Manager</title>
</head>
<body>
    <h2>Event Manager</h2>
    <h4>@Html.DisplayNameFor(model => model.Title)</h4>
    <ul>
        @foreach (var listEvent in Model)
        {
            <li>@Html.DisplayFor(modelEvent => listEvent.Title) | @Html.ActionLink("Details", "Details", new { id = listEvent.EventId })</li>
        }
    </ul>
</body>
</html>